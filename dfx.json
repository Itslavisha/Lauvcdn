{
  "version": 1,
  "canisters": {
    "registry": {
      "type": "custom",
      "candid": "canisters/registry/registry.did",
      "wasm": "target/wasm32-unknown-unknown/release/registry.wasm",
      "build": "cargo build --target wasm32-unknown-unknown --release --package registry"
    },
    "storage": {
      "type": "custom",
      "candid": "canisters/storage/storage.did",
      "wasm": "target/wasm32-unknown-unknown/release/storage.wasm",
      "build": "cargo build --target wasm32-unknown-unknown --release --package storage"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  }
}